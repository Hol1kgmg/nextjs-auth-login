{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "panda codegen",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm-run-all2 -p lint:*",
    "lint:code": "oxlint --type-aware --type-check --fix && eslint --fix",
    "lint:markup": "markuplint \"src/**/*.{html,jsx,tsx}\" --fix --problem-only",
    "format": "oxfmt",
    "check": "npm-run-all2 lint format",
    "test": "npm-run-all2 -p test:*",
    "test:unit": "vitest --project=unit",
    "test:storybook": "vitest --project=storybook",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "better-auth": "^1.4.18",
    "next": "16.1.4",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@markuplint/jsx-parser": "^4.7.22",
    "@markuplint/react-spec": "^4.5.22",
    "@pandacss/dev": "^1.8.1",
    "@pandacss/eslint-plugin": "^0.3.0",
    "@storybook/addon-a11y": "^10.2.0",
    "@storybook/addon-docs": "^10.2.0",
    "@storybook/addon-onboarding": "^10.2.0",
    "@storybook/addon-themes": "^10.2.0",
    "@storybook/addon-vitest": "^10.2.0",
    "@storybook/nextjs-vite": "^10.2.0",
    "@testing-library/react": "^16.3.2",
    "@tsconfig/strictest": "^2.0.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-command": "^3.4.0",
    "eslint-plugin-oxlint": "^1.41.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.8.5",
    "eslint-plugin-storybook": "^10.2.0",
    "jsdom": "^27.4.0",
    "lefthook": "^2.0.15",
    "markuplint": "^4.14.0",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "oxlint-tsgolint": "^0.11.1",
    "playwright": "^1.57.0",
    "storybook": "^10.2.0",
    "typescript": "^5",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
